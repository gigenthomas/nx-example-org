{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-example-org/capability/issues": [
        "packages/capability/issues/src/index.ts"
      ],
      "@nx-example-org/issues": ["packages/issues/src/index.ts"],
      "@nx-example-org/prolog-shared-models": [
        "packages/prolog-shared-models/src/index.ts"
      ],
      "@nx-example-org/ui-shared": ["packages/ui-shared/src/index.ts"],
      "@nx-example-org/ui-shared-layout": [
        "packages/ui-shared-layout/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
